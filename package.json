{
  "name": "md-dynamic-forms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run build:core && npm run build:material",
    "start": "ng serve",
    "build": "ng build",
    "build:app": "ng build md-dynamic-forms-app --prod",
    "build:core": "ng build md-dynamic-forms-core --prod",
    "build:core:watch": "ng build md-dynamic-forms-core --prod --watch",
    "build:material": "ng build md-dynamic-forms-material --prod",
    "build:material:watch": "ng build md-dynamic-forms-material --prod --watch",
    "test": "ng test",
    "test:core": "ng test md-dynamic-forms-core --code-coverage",
    "test:material": "ng test md-dynamic-forms-material --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.11",
    "@angular/cdk": "^11.2.10",
    "@angular/common": "~11.2.11",
    "@angular/compiler": "~11.2.11",
    "@angular/core": "~11.2.11",
    "@angular/forms": "~11.2.11",
    "@angular/material": "^11.2.10",
    "@angular/material-moment-adapter": "^11.2.10",
    "@angular/platform-browser": "~11.2.11",
    "@angular/platform-browser-dynamic": "~11.2.11",
    "@angular/router": "~11.2.11",
    "moment": "^2.29.1",
    "ngx-logger": "^4.2.1",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.10",
    "@angular/cli": "~11.2.10",
    "@angular/compiler-cli": "~11.2.11",
    "@angular/language-service": "~11.2.11",
    "@ngneat/spectator": "^7.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-mocks": "^11.10.1",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
